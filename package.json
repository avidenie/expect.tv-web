{
  "name": "@expect.tv/web",
  "version": "0.1.0",
  "private": true,
  "description": "A suite of apps and tools to discover great movies and TV shows",
  "bugs": {
    "url": "https://github.com/avidenie/expect.tv-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avidenie/expect.tv-web.git"
  },
  "author": "Adrian Videnie <avidenie@gmail.com>",
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "commit": "git-cz",
    "release": "standard-version",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@emotion/core": "^10.0.21",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "querystringify": "^2.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addon-viewport": "^5.2.4",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  }
}
